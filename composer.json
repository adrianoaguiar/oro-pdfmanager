{
    "name": "marello/orocrm-pdfmanager",
    "description": "Ibnab PDF Manager",
    "homepage": "https://github.com/marellocommerce/orocrm-pdfmanager.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Ibnab",
            "homepage": "http://www.ibnab.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Ibnab\\Bundle\\PmanagerBundle": "src/"
        }
    },
    "target-dir": "src/Ibnab/Bundle/PmanagerBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.4.9",
        "oro/platform": "1.8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}

